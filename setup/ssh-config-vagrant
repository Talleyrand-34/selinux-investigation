Host default
  HostName 192.168.121.218
  User vagrant
  Port 22
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile /home/asada/.vagrant.d/insecure_private_key
  IdentitiesOnly yes
  LogLevel FATAL


